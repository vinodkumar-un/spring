<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="chip1" class="com.autoanno.test.Chip">
		<property name="id" value="25" />
		<property name="type" value="Silicon" />
	</bean>
	
	<bean id="chip2" class="com.autoanno.test.Chip">
		<property name="id" value="15" />
		<property name="type" value="Electrical" />
	</bean>

	<!-- <bean id="sensor" class="com.autoanno.test.Sensor">
		<property name="id" value="35" />
		<property name="range" value="10" />
	</bean> -->

	<bean id="robot" class="com.autoanno.test.Robot" />
	<context:annotation-config />
</beans>






