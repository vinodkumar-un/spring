<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="address" name="address1" class="com.ba.beans.Address">
		<property name="addressLine1" value="505, 5th floor" />
		<property name="addressLine2" value="ameerpet" />
		<property name="city" value="hyderabad" />
		<property name="state" value="TS" />
		<property name="zip" value="3535" />
		<property name="country" value="India" />
	</bean>
	<alias name="address1" alias="505,Mithrivanam"/>
</beans>








