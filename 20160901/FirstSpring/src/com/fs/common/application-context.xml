<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="messageWriter" class="com.fs.beans.MessageWriter">
		<property name="messageProducer" ref="pdfMessageProducer" />
	</bean>
	
	<bean id="htmlMessageProducer" class="com.fs.beans.HtmlMessageProducerImpl" />
	<bean id="pdfMessageProducer" class="com.fs.beans.PdfMessageProducerImpl" />

</beans>







